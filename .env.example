# NetROS Environment Configuration
# Kopier denne filen til .env og fyll inn verdier

# Database
# For utvikling (SQLite):
DATABASE_URL=sqlite+aiosqlite:///./netros.db

# For produksjon (PostgreSQL):
# DATABASE_URL=postgresql+asyncpg://netros:password@localhost:5432/netros

# Sikkerhet
SECRET_KEY=endre-denne-til-en-lang-tilfeldig-streng-i-produksjon
ACCESS_TOKEN_EXPIRE_MINUTES=480
SESSION_EXPIRE_HOURS=8

# Applikasjon
APP_NAME=NetROS
APP_VERSION=0.1.0
DEBUG=true
LOG_LEVEL=INFO

# Netbox (Fase 2)
# NETBOX_URL=https://netbox.neas.no
# NETBOX_TOKEN=ditt-netbox-api-token
# NETBOX_SYNC_INTERVAL_HOURS=24

# E-post (Fase 3)
# SMTP_HOST=smtp.neas.no
# SMTP_PORT=587
# SMTP_USER=netros@neas.no
# SMTP_PASSWORD=passord
# SMTP_FROM=netros@neas.no

# Rapport-innstillinger
COMPANY_NAME=NEAS AS
COMPANY_ORG_NR=123456789
